<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9</TargetFramework>
    <Nullable>enable</Nullable>
    <OutputType>Exe</OutputType>
    <RootNamespace>NAPS2.Tools</RootNamespace>
    <AssemblyName>n2</AssemblyName>
    <Configurations>Debug</Configurations>
    <LangVersion>11</LangVersion>
    <EnablePreviewFeatures>true</EnablePreviewFeatures>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.9.1" />
    <PackageReference Include="Crowdin.Api" Version="2.24.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="NuGet.Protocol" Version="6.10.1" />
    <PackageReference Include="Octokit" Version="13.0.1" />
    <PackageReference Include="SSH.NET" Version="2024.1.0" />
    <PackageReference Include="System.Security.Principal.Windows" Version="5.0.0" />
    <PackageReference Include="VirusTotalNet" Version="2.2.0" />
  </ItemGroup>

  <Import Project="..\NAPS2.Setup\targets\CommonTargets.targets" />
  
</Project>